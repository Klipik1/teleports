        ["o"] = {"Mysterious Arrow"}
    }
elseif workspace.Roka.Value ~= 25 then
    getgenv().w = {
        ["o"] = {"Rokakaka"}
    }
end
    while wait() do
        for _, aga in pairs(workspace.Item_Spawns.Items:GetChildren()) do
            if aga:IsA("Model") then
                for _, aga2 in pairs(aga:GetChildren()) do
                if aga2:IsA("MeshPart") then
                    if aga:FindFirstChild("ProximityPrompt") then
                        if table.find(w["o"], aga.ProximityPrompt.ObjectText) and aga2.Transparency ~= 1 then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = aga:GetModelCFrame()
                            wait(0.5)
                        if not table.find(w["o"], aga.ProximityPrompt.ObjectText) and aga2.Transparenct ~= 1 then
                            game.TeleportService:Teleport(game.PlaceId)
                        end
                        if aga:FindFirstChild("ProximityPrompt") then
                                fireproximityprompt(aga.ProximityPrompt)
                                wait(1.5)
                        end
                    if LocalPlayer.PlayerGui:FindFirstChild("Message") then
                        game.TeleportService:Teleport(game.PlaceId)
                    end
                end
            end
        end
                end
            end
end
