    LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-801.063, -42.5264, -861.054)
    task.wait(0.3)
    if workspace.Roka.Value <= 1 or workspace.Roka.Value <= 1 then
        sendWebhook("Закончились роки и стрелы для дальнейшего фарма")
        break
    end
    if table.find(wat["watafac"], LocalPlayer.PlayerStats.Stand.Value) then
        sendWebhook("У вас уже имеется: " .. LocalPlayer.PlayerStats.Stand.Value .. ". Сервер айди был скопирован")
        setclipboard(game.JobId)
        ca.Font=Enum.Font.SourceSansSemibold;ca.Text=LocalPlayer.PlayerStats.Stand.Value ca.TextColor3=Color3.new(0,1,1)
        ca.TextSize=33;da.Parent=ca
        break
    end
    ca.Font=Enum.Font.SourceSansSemibold;ca.Text=LocalPlayer.PlayerStats.Stand.Value ca.TextColor3=Color3.new(0,1,1)
    ca.TextSize=33;da.Parent=ca
    if LocalPlayer.PlayerStats.Stand.Value == "None" then
    wait(1)
    UseItem(arr)
    repeat wait() until LocalPlayer.PlayerStats:WaitForChild'Stand'.Value ~= "None"
    wait(0.5)
    ca.Font=Enum.Font.SourceSansSemibold;ca.Text=LocalPlayer.PlayerStats.Stand.Value ca.TextColor3=Color3.new(0,1,1)
    ca.TextSize=33;da.Parent=ca
    if table.find(wat["watafac"], LocalPlayer.PlayerStats.Stand.Value) then
        sendWebhook("Вы получили: " .. LocalPlayer.PlayerStats.Stand.Value .. ". Сервер айди был скопирован")
        setclipboard(game.JobId)
        ca.Font=Enum.Font.SourceSansSemibold;ca.Text=LocalPlayer.PlayerStats.Stand.Value ca.TextColor3=Color3.new(0,1,1)
        ca.TextSize=33;da.Parent=ca
        break
    else
    print("Unwanted Stand: "..LocalPlayer.PlayerStats.Stand.Value)
    end
    UseItem(roka)
    wait(4.12)
else
    UseItem(roka)
    wait(4.12)
end
end
end
if workspace.Arrow.Value ~= 25 and workspace.Roka.Value ~= 25 then
    getgenv().w = {
        ["o"] = {"Rokakaka", "Mysterious Arrow"}
}
elseif workspace.Arrow.Value ~= 25 then
