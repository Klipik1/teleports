repeat task.wait() until game:IsLoaded()
local LocalPlayer = game:GetService("Players").LocalPlayer
local workspace = game:GetService("Workspace")
old = hookmetamethod(game,"__namecall", function(self,...)
    local args = {...}
    local method = getnamecallmethod()
    if tostring(self) == "Returner" and tostring(method) == "InvokeServer" then
       return "  ___XP DE KEY"
    end
    return old(self, ...)
end)
local WD = "InputBegan"
local DW = {["Input"] = Enum.KeyCode.E}
local KEK = {["Input"] = Enum.KeyCode.T}
local aw = {["Input"] = Enum.KeyCode.R}
local FACA = {["Input"] = Enum.KeyCode.G}
local shhhit = "Dash"
task.wait(7)
local Event = workspace.Living:WaitForChild(LocalPlayer.Name).RemoteEvent
if game.PlaceId == 2809202155 then
    repeat task.wait()
    until LocalPlayer.PlayerGui:FindFirstChild("LoadingScreen")
    task.wait(3)
    while task.wait(8) do
        if LocalPlayer.PlayerGui:FindFirstChild("Metal Ball Run Servers") then break end
        firesignal(LocalPlayer.PlayerGui.LoadingScreen.Frames.Main.Play.MouseButton1Click)
        firesignal(LocalPlayer.PlayerGui.LoadingScreen.Frames.SteelBallRun.Ranked.Play.MouseButton1Click)
        if LocalPlayer.PlayerGui:FindFirstChild("Metal Ball Run Servers") then break end
    end
    for _, aw in pairs(LocalPlayer.PlayerGui:WaitForChild("Metal Ball Run Servers").Frame.ScrollingFrame:GetChildren()) do
        if aw:IsA("TextButton") then
            firesignal(aw.MouseButton1Click)
            firesignal(aw.MouseButton1Click)
            firesignal(aw.MouseButton1Click)
        end
    end
end
if game.PlaceId == 4643697430 then
    local N=game:GetService("VirtualInputManager")
    N:SendKeyEvent(true,"LeftControl",false,game)
    local function Hamon()
        local uwu = "LearnSkill" local awa = {["Skill"] = "Hamon Breathing V", ["SkillTreeType"] = "Spec"} local waw = {["Skill"] = "Hamon Punch", ["SkillTreeType"] = "Spec"} local hah = {["Skill"] = "Lung Capacity V", ["SkillTreeType"] = "Spec"}
        Event:FireServer(uwu, awa) Event:FireServer(uwu, hah) Event:FireServer(uwu, waw)
        if workspace.Living[LocalPlayer.Name].HumanoidRootPart:FindFirstChild("Burning2") then
            Event:FireServer(shhhit)
        end
        Event:FireServer(WD, FACA)
    end
    local function wata()
        if not workspace.Living[LocalPlayer.name]:FindFirstChild("StandMorph") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1096.88, 267.315, -887.613)
            task.wait(1)
            repeat task.wait()
            local N=game:GetService("VirtualInputManager")
            N:SendKeyEvent(true,"Q",false,game)
            until workspace.Living[LocalPlayer.name]:FindFirstChild("StandMorph")
        end
    end
    if workspace:FindFirstChild("Message") then
        task.wait(0.2)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1096.88, 267.315, -887.613)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1096.88, 267.315, -887.613)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1096.88, 267.315, -887.613)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1096.88, 267.315, -887.613)
        repeat wait()
            local time = 145
            while task.wait(1) do
                if time == 1 then
                    game:GetService("TeleportService"):Teleport(2809202155)
                end
                if not workspace:FindFirstChild("Message") then
                    break
                end
                time = time - 1
                print(time)
            end
            until not workspace:FindFirstChild("Message")
            end
    if not workspace:FindFirstChild("Message") then
        task.wait(0.1)
        local color = BrickColor.new(50,0,250)
        local transparency = .8

        local Players = game:GetService("Players")
        local function _ESP(c)
        repeat wait() until c.PrimaryPart ~= nil
        for i,p in pairs(c:GetChildren()) do
        if p.ClassName == "Part" or p.ClassName == "MeshPart" then
            if p:FindFirstChild("shit") then p.shit:Destroy() end
            local a = Instance.new("BoxHandleAdornment",p)
            a.Name = "shit"
            a.Size = p.Size
            a.Color = color
            a.Transparency = transparency
            a.AlwaysOnTop = true    
            a.Visible = true    
            a.Adornee = p
            a.ZIndex = true    

        end
        end
        end
        local function ESP()
        for i,v in pairs(Players:GetChildren()) do
        if v ~= game.Players.LocalPlayer then
            if v.Character then
            _ESP(v.Character)
            end
            v.CharacterAdded:Connect(function(chr)
            _ESP(chr)
            end)
        end
        end
        Players.PlayerAdded:Connect(function(player)
        player.CharacterAdded:Connect(function(chr)
            _ESP(chr)
        end)  
        end)
        end
        ESP()
        N:SendKeyEvent(true,"Q",false,game)
        for _, wa in pairs(workspace.Living:GetChildren()) do
                if wa:IsA("Model") then
                    for _, wa2 in pairs(wa:GetChildren()) do
                        if wa:FindFirstChild("HumanoidRootPart") then
                            if wa.Name ~= LocalPlayer.Name then
                                if wa.Humanoid.Health > 1 then
                                    task.wait(1)
                                    while task.wait() do
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                        wata()
                                        workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph"):WaitForChild("HumanoidRootPart").CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                        wata()
                                        workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph").HumanoidRootPart.CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                        Event:FireServer(WD, DW)
                                        Hamon()
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                        wata()
                                        workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph").HumanoidRootPart.CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                        Event:FireServer(WD, aw)
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                        wata()
                                        workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph").HumanoidRootPart.CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                        Event:FireServer(WD, KEK)
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                        wata()
                                        workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph").HumanoidRootPart.CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                        if game.Players.numPlayers < 5 then
                                            game:GetService("TeleportService"):Teleport(2809202155)
                                        end
                                        for _, wa in pairs(workspace.Living:GetChildren()) do
                                            if wa.Humanoid.Health > 1 then
                                                if wa.Name ~= LocalPlayer.Name then
                                                    LocalPlayer.Character.HumanoidRootPart.CFrame = wa.HumanoidRootPart.CFrame + Vector3.new(0,-40,0)
                                                    wata()
                                                    workspace.Living[LocalPlayer.name]:WaitForChild("StandMorph").HumanoidRootPart.CFrame = CFrame.new(wa.HumanoidRootPart.Position)
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
